<p><h1>Welcome to WCESQUARE</h1></p>
<h3><% provide(:title, @user.name) %></h3>
<h1>
  <%= gravatar_for @user %>
  <%= @user.name %>
</h1>
<div class="col-md-8">
    <% if @user.microposts.any? %>
      <h3>Books (<%= @user.microposts.count %>)</h3>
      <ol class="micropost">
        <%= render @microposts %>
      </ol>
      <%= will_paginate @microposts %>
    <% end %>
  </div>
